<!-- https://microbialcellfactories.biomedcentral.com/articles/10.1186/s12934-019-1073-5/tables/2 -->
<!-- Michaelis-Menten kinetics with apparent saturation constant KM = 1.75 microM, 
and maximum 2-NBDG uptake rate=197 molecules/cell-second. PMID:10937825 DOI:10.1006/mben.1999.0125-->
<!-- https://bionumbers.hms.harvard.edu/bionumber.aspx?s=n&v=5&id=111713 -->

<intracellular type="dfba">
    <sbml_filename>path/to/model.sbml</sbml_filename>
    <exchange substrate="glucose">
        <fba_flux>EX_glc_e</fba_flux>
        <Km units="microM">1.75</Km>
        <Vmax units="molecules/cell/seconds">197</Vmax>
    </exchange>
    <exchange substrate="oxygen">
        <fba_flux>EX_o2_e</fba_flux>
        <Km units="microM">1</Km>
        <Vmax units="molecules/cell/seconds">300</Vmax>
    </exchange>
    <exchange substrate="lactate">
        <fba_flux>EX_lac_e</fba_flux>
        <Km units="microM">1</Km>
        <Vmax units="molecules/cell/seconds">100</Vmax>
    </exchange>
</intracellular>

